  - campaign_ids
  - ad_set_ids
  - ads_published_count
  - launch_status                 # planned | live | frozen | stabilized
  - diagnostics_snapshot          # EMQ p50, event latency p50, learning states
kpis:
  freeze_window_hours: ">= 48 & <= 72"
  emq_target: ">= 5"
  capi_latency_p50_seconds: "<= 300"
  events_per_adset_week: ">= 50"         # pace to exit Learning Limited
  session_roas_gate: ">= 1.30x"          # evaluated post-freeze
guards:
  - "Do not launch if Purchase (value) is not rank #1 in AEM."
  - "Do not launch if CAPI Purchase(value) test event not received with EMQ ≥ 5."
  - "No structural edits during the 48–72h freeze window (freeze_window)."
  - "Terminal must run in dryRun=true during freeze_window."
  - "Do not exceed 15 ads per ad set at launch (fragmentation guard)."
  - "Do not point stability lanes to more than 3 LPIDs at launch."
